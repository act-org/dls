  "repository": {
    "type": "git",
    "url": "git+https://github.com/act-org/dls.git"
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:clean": "rimraf dist/*",
    "build:components": "tsc --project tsconfig.build.json",
    "build:icons": "bin/buildIcons",
    "build:copy": "npm run copy",
    "build-storybook": "build-storybook -o ./storybook-static/www",
    "build-storybook:dockerprep": "build-storybook -o ./storybook-static/www && cp Dockerfile ./storybook-static/Dockerfile",
    "build-storybook:docker": "cd storybook-static && docker build . -t dls-storybook",
    "copy": "npm-run-all copy:*",
    "copy:changelog": "cp -rf CHANGELOG.md dist",
    "copy:license": "cp -rf LICENSE dist",
    "copy:package": "cp -rf package.json dist",
    "copy:packageclean": "clear-package-json dist/package.json -o dist/package.json",
    "copy:readme": "cp -rf README.md dist",
    "postversion": "npm run copy",
    "prepare": "husky install",
    "release": "npm-run-all release:**",
    "release:version": "standard-version",
    "release:commit": "git push --follow-tags origin main",
    "release:npm": "npm run build && cd dist && npm publish",
    "release:storybook": "npm run build-storybook && touch ./storybook-static/www/.nojekyll && storybook-to-ghpages -e ./storybook-static/www",
    "start": "start-storybook -p 6006",
    "test": "npm-run-all test:**",
    "test:lint:js": "eslint . --ext .js --fix --quiet",
    "test:lint:md": "markdownlint *.md --config node_modules/@actinc/eslint-config/markdownlint.config.json",
    "test:lint:ts": "eslint . --ext .ts,.tsx --fix --quiet",
    "test:types": "tsc --project tsconfig.build.json --noEmit",
    "test:unit": "jest --maxWorkers=2 --verbose"
  },
  "types": "index.d.ts",
  "version": "6.3.0"
}
