  "lint-staged": {
    "*.js": "npm run test:lint:js",
    "*.{js,json,md,mdx,ts,tsx}": "prettier --write",
    "*.{ts,tsx}": "npm run test:lint:ts",
    "*.md": "npm run test:lint:md"
  },
  "main": "index.js",
  "name": "@actinc/dls",
  "peerDependencies": {
    "@emotion/react": "11.x",
    "@emotion/styled": "11.x",
    "@mui/lab": "^5.0.0-alpha.82",
    "@mui/material": "5.x",
    "@mui/system": "5.x",
    "@mui/x-data-grid": "5.x || 6.x",
    "react": "17.x || 18.x",
    "react-dom": "17.x || 18.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md",
          "changeLogTitle": "# Changelog"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "npm run copy && npm version ${nextRelease.version} --no-commit-hooks --no-git-tag-version"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./dist"
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "repositoryUrl": "git@github.com:act-org/dls.git",
    "branches": [
      {
        "name": "main",
        "prerelease": false
      },
      {
        "name": "next",
        "prerelease": true
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/act-org/dls.git"
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:clean": "rimraf dist/*",
    "build:components": "tsc --project tsconfig.build.json",
    "build:icons": "bin/buildIcons",
    "build:package_json": "clear-package-json package.json -o dist/package.json",
    "build-storybook": "build-storybook -o ./storybook-static/www",
    "copy": "npm-run-all copy:*",
    "copy:changelog": "cp -rf CHANGELOG.md dist",
    "copy:license": "cp -rf LICENSE dist",
    "copy:readme": "cp -rf README.md dist",
    "prepare": "husky install",
    "release:storybook": "npm run build-storybook && touch ./storybook-static/www/.nojekyll && storybook-to-ghpages -e ./storybook-static/www",
    "start": "start-storybook -p 6006",
    "test": "npm-run-all test:**",
    "test:lint:js": "eslint . --ext .js --fix --quiet",
    "test:lint:md": "markdownlint *.md --config node_modules/@actinc/eslint-config/markdownlint.config.json",
    "test:lint:ts": "eslint . --ext .ts,.tsx --fix --quiet",
    "test:types": "tsc --project tsconfig.build.json --noEmit",
    "test:unit": "jest --maxWorkers=2 --verbose"
  },
  "types": "index.d.ts",
  "version": "7.0.1"
}
